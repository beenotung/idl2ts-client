{
  "name": "idl2ts-client",
  "version": "1.3.1",
  "description": "http library for typescript file generated from idl2ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "lint": "tslint -p .",
    "format": "prettier --write \"src/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "test": "run-s build lint"
  },
  "keywords": [
    "http",
    "idl2ts"
  ],
  "author": "Beeno Tung",
  "license": "BSD-2-Clause",
  "peerDependencies": {},
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^10.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/idl2ts-ng.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/idl2ts-ng/issues"
  },
  "homepage": "https://github.com/beenotung/idl2ts-ng#readme",
  "dependencies": {
    "axios": "^0.18.0"
  }
}
